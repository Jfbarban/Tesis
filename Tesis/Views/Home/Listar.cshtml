@model List<string>

<head>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="~/js/ListaScript.js"></script>
    <link rel="stylesheet" href="~/css/ListarSteel.css" />
</head>

<body>
    <header>
        <h2>Lista Direcciones</h2>
    </header>

    <div>
        <ul>
            @foreach (var mensaje in Model)
            {
                <li>
                    <a href="@mensaje" target="_blank">@mensaje</a>
                </li>
            }
        </ul>
        <div>
            <button id="btnBorrar" class="btnBorrar">Borrar Lista</button>
        </div>
    </div>
    

</body>